	"keywords": [
		"nette",
		"javascript",
		"webpack",
		"webpack-encore"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Lukas Povoda",
			"email": "lupo112@gmail.com"
		}
	],
	"require": {
		"php": ">= 8.0",
		"ext-json": "*",
		"nette/application": "^3.1",
